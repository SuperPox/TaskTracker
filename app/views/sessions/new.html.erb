<div class="container">
    <br>

<h2>Log In</h2>

  <% @errors && @errors.each do |e| %>
    <li><%=e%></li>
  <%end%>

<%= form_for :user do |f|%>
  <%=f.label "Username" %>
  <%=f.text_field :username%><br>
  <%=f.label "Password"%>
  <%=f.password_field :password %>
  <br>
  <%=f.submit "Log In"%>
  <br>
  <br>
  <%=link_to "Sign Up With Facebook", '/auth/facebook'%>

<%end%>
</div>